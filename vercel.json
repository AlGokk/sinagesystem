{
  "version": 2,
  "builds": [
    {
      "src": "build/backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/ui/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/build/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/ui/$1"
    }
  ]
}
